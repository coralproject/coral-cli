{
  "name": "@coralproject/coral-cli",
  "description": "CLI to interact with Coral",
  "version": "0.5.0",
  "author": "Wyatt Johnson @wyattjoh",
  "bin": {
    "coral-cli": "./bin/run"
  },
  "bugs": "https://github.com/coralproject/coral-cli/issues",
  "dependencies": {
    "@coralproject/coral-cli-command": "^0.4.0",
    "@heroku-cli/color": "^1.1.14",
    "@oclif/command": "^1.8.0",
    "@oclif/config": "^1.17.0",
    "@oclif/plugin-help": "^3.2.1",
    "@oclif/plugin-not-found": "^1.2.4",
    "@oclif/plugin-plugins": "^1.9.5",
    "cli-ux": "^5.5.1",
    "isomorphic-fetch": "^3.0.0",
    "lodash": "^4.17.20",
    "netrc-parser": "^3.1.6"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.26.0",
    "@types/isomorphic-fetch": "0.0.35",
    "@types/lodash": "^4.14.166",
    "@types/node": "^14.14.16",
    "@types/supports-color": "^7.2.0",
    "@typescript-eslint/eslint-plugin": "^4.11.1",
    "@typescript-eslint/parser": "^4.11.1",
    "eslint": "^7.16.0",
    "eslint-config-prettier": "^7.1.0",
    "eslint-plugin-prettier": "^3.3.0",
    "globby": "^11.0.1",
    "husky": "^4.3.6",
    "lint-staged": "^13.0.0",
    "prettier": "^2.2.1",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3"
  },
  "engines": {
    "node": "^13.11"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/coralproject/coral-cli",
  "keywords": [
    "oclif"
  ],
  "license": "Apache-2.0",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "coral-cli",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-not-found",
      "@oclif/plugin-plugins"
    ],
    "npmRegistry": "https://npm.pkg.github.com/coralproject",
    "topics": {
      "story": {
        "description": "manage stories on Coral"
      }
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "repository": "coralproject/coral-cli",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "pack": "oclif-dev pack",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "lint": "eslint .",
    "test": "eslint . && tsc -p . --noEmit",
    "version": "oclif-dev readme && git add README.md"
  },
  "types": "lib/index.d.ts"
}
